# platform :ios, '9.0'

source 'git@github.com:hudl/podspecs.git'
source 'https://github.com/CocoaPods/Specs.git'

target 'URLRouter+ReactNative' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  use_frameworks!

  # Pods for URLRouter+ReactNative
  pod 'URLRouter', :git => 'https://github.com/aichamorro/URLRouter', :branch => 'master'
  pod 'React/Core'
  pod 'React/RCTText'
  pod 'React/RCTImage'
  pod 'React/RCTNetwork'

end
